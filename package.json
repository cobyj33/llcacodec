{
    "name": "llcacodec.js",
    "description": "Read and Write Life-Like Cellular Automata Files",
    "version": "0.1.0",
    "author": {
        "name": "Jacoby Johnson",
        "email": "jacobyajohnson@gmail.com",
        "url": "https://www.github.com/cobyj33"
    },
    "keywords": [
        "automata",
        "conway",
        "cellular-automata",
        "life-like"
    ],
    "exports": {
        ".": {
            "import": {
                "default": "./dist/api.js"
            },
            "types": {
                "default": "./dist/types/api.d.ts"
            }
        },
        "./rule": {
            "import": {
                "default": "./dist/formats/rule.js"
            },
            "types": {
                "default": "./dist/types/formats/rule.d.ts"
            }
        }
    },
    "typesVersions": {
        "*": {
            "rule": [
                "./dist/types/formats/rule.d.ts"
            ]
        }
    },
    "main": "./dist/api.js",
    "types": "./dist/types/api.d.ts",
    "devDependencies": {
        "@babel/core": "^7.21.0",
        "@babel/plugin-transform-modules-commonjs": "^7.21.2",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-typescript": "^7.21.0",
        "@types/jest": "^29.4.0",
        "babel-jest": "^29.4.3",
        "jest": "^29.4.3",
        "jest-expect-message": "^1.1.3",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    },
    "scripts": {
        "test": "jest --testPathIgnorePatterns=tests/brute",
        "testFull": "jest",
        "clearJestCache": "jest --clearCache"
    }
}
